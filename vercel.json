{
  "version": 2,
  "builds": [
    {
      "src": "/server/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/server/src"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/client/index.html"
    }
  ]
}
